            android:layout_height="130dp"
            android:layout_weight="1"
            android:background="#eff0f1"
            android:orientation="horizontal"
            android:weightSum="3"
            tools:layout_editor_absoluteX="9dp"
            tools:layout_editor_absoluteY="13dp">

            <LinearLayout
